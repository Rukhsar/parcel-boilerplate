{
  "name": "parcel-boilerplate",
  "version": "1.0.0",
  "description": "This is a starter workflow for building vanilla JavaScript applications using Parcel along with Babel and Sass.",
  "main": "index.js",
  "scripts": {
    "dev": "parcel public/index.html --out-dir development -p 3000",
    "build": "parcel build public/index.html --out-dir dist --public-url ./"
  },
  "author": "Rukhsar Manzoor",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "any": "^1.0.0"
  }
}
